    "slugify"
  ],
  "author": "Paul Smith <paulsmith@pobox.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulsmith/angular-slugify/issues"
  },
  "homepage": "https://github.com/paulsmith/angular-slugify",
  "dependencies": {
    "angular": "^1.0.x"
  },
  "devDependencies": {
    "karma": "^0.13.3",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "bower": "^1.4.1",
    "angular": "^1.2.x",
    "angular-mocks": "^1.2.x"
  }
}
